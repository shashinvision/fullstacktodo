emplate>\n\n<script>\nimport { BRow, BCol, BButton } from \"bootstrap-vue\";\n\nexport default {\n  name: \"TasksList\",\n  components: {\n    BRow,\n    BCol,\n    BButton,\n  },\n  data() {\n    return {\n      item: 0,\n      dataList: \"\",\n    };\n  },\n  methods: {\n    async taskList() {\n      const URL_FETCH_ALL = \"http://localhost:8888/api/task\";\n      await fetch(URL_FETCH_ALL, {\n        method: \"GET\",\n      })\n        .then((res) => {\n          return res.json();\n        })\n        .then((data) => {\n          // console.log(\"URL\", data);\n          this.dataList = data;\n          // console.log(\"URL\", this.dataList);\n        })\n        .catch(function (err) {\n          console.error(err);\n        });\n    },\n  },\n  mounted() {\n    this.taskList();\n  },\n};\n</script>\n\n<style lang=\"css\" scoped>\n.b-icon:hover {\n  cursor: pointer;\n  opacity: 0.6;\n}\n</style>\n"],"sourceRoot":"src/components"}]}