table\">\n          <thead>\n            <tr>\n              <th scope=\"col\">#</th>\n              <th scope=\"col\">Title</th>\n              <th scope=\"col\">Description</th>\n              <th scope=\"col\">Detail</th>\n              <th scope=\"col\">Edit</th>\n              <th scope=\"col\">Drop</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"item in dataList\" :key=\"item.id\">\n              <th scope=\"row\">{{ item.id }}</th>\n              <td>{{ item.title }}</td>\n              <td>{{ item.description }}</td>\n              <td>\n                <b-iconstack font-scale=\"1\">\n                  <b-icon\n                    stacked\n                    icon=\"box-arrow-up-right\"\n                    variant=\"primary\"\n                  ></b-icon>\n                </b-iconstack>\n              </td>\n              <td>\n                <b-iconstack font-scale=\"1\">\n                  <b-icon stacked icon=\"pen\" variant=\"success\"></b-icon>\n                </b-iconstack>\n              </td>\n              <td>\n                <b-iconstack font-scale=\"1\">\n                  <b-icon stacked icon=\"trash\" variant=\"danger\"></b-icon>\n                </b-iconstack>\n              </td>\n            </tr>\n          </tbody>\n        </table>\n      </BCol>\n    </BRow>\n  </div>\n</template>\n\n<script>\nimport { BRow, BCol, BButton } from \"bootstrap-vue\";\n\nexport default {\n  name: \"TasksList\",\n  components: {\n    BRow,\n    BCol,\n    BButton,\n  },\n  data() {\n    return {\n      item: 0,\n      dataList: \"\",\n    };\n  },\n  methods: {\n    async taskList() {\n      const URL_FETCH_ALL = \"http://localhost:8888/api/task\";\n      await fetch(URL_FETCH_ALL, {\n        method: \"GET\",\n      })\n        .then((res) => {\n          return res.json();\n        })\n        .then((data) => {\n          // console.log(\"URL\", data);\n          this.dataList = data;\n          // console.log(\"URL\", this.dataList);\n        })\n        .catch(function (err) {\n          console.error(err);\n        });\n    },\n  },\n  mounted() {\n    this.taskList();\n  },\n};\n</script>\n\n<style lang=\"css\" scoped>\n.b-icon:hover {\n  cursor: pointer;\n  opacity: 0.6;\n}\n</style>\n"]}]}